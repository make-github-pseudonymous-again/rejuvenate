{
  "name": "rejuvenate",
  "description": "Keep your package up to date",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "author": "make-github-pseudonymous-again",
  "homepage": "https://make-github-pseudonymous-again.github.io/rejuvenate",
  "repository": {
    "type": "git",
    "url": "https://github.com/make-github-pseudonymous-again/rejuvenate"
  },
  "bugs": {
    "url": "https://github.com/make-github-pseudonymous-again/rejuvenate/issues"
  },
  "keywords": [
    "codemod",
    "dev",
    "esnext",
    "patch",
    "typescript",
    "update"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src --out-dir dist --source-maps",
    "build-gh-pages": "npm run build-docs",
    "commit-msg": "commitlint --edit",
    "install-hooks": "husky install",
    "lint": "xo",
    "lint-and-fix": "npm run lint -- --fix",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np --message ':hatching_chick: chore(release): v%s'",
    "tsc": "tsc"
  },
  "dependencies": {
    "@async-abstraction/tape": "^9.0.0",
    "@babel/parser": "^7.16.6",
    "@data-structure/deque": "^9.0.1",
    "@graph-algorithm/topological-sorting": "^1.0.0",
    "@iterable-iterator/cardinality": "^4.0.0",
    "@iterable-iterator/filter": "^1.0.1",
    "@iterable-iterator/iter": "^1.0.1",
    "@iterable-iterator/list": "^1.0.1",
    "@iterable-iterator/map": "^1.0.1",
    "@iterable-iterator/reduce": "^1.0.1",
    "@iterable-iterator/slice": "^1.0.1",
    "@iterable-iterator/zip": "^1.0.1",
    "@total-order/primitive": "^1.0.1",
    "chalk": "^5.0.0",
    "cli-truncate": "^3.1.0",
    "command-exists": "^1.2.9",
    "del": "^6.0.0",
    "elegant-spinner": "^3.0.0",
    "execa": "^6.0.0",
    "fast-glob": "^3.2.7",
    "figures": "^4.0.0",
    "find-up": "^6.2.0",
    "indent-string": "^5.0.0",
    "listr": "^0.14.3",
    "load-json-file": "^7.0.1",
    "log-symbols": "^5.1.0",
    "log-update": "^5.0.0",
    "make-dir": "^3.1.0",
    "move-file": "^3.0.0",
    "npm-check-updates": "^12.0.5",
    "pkg-dir": "^6.0.1",
    "power-assert": "1.6.1",
    "recast": "^0.20.5",
    "simple-git": "^2.48.0",
    "strip-ansi": "^7.0.1",
    "write-json-file": "^5.0.0",
    "yaml": "^2.0.0-9",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/yargs": "^17.0.7",
    "babel-preset-power-assert": "3.0.0",
    "fixpack": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.3",
    "np": "7.6.0",
    "pinst": "2.1.6",
    "regenerator-runtime": "0.13.9",
    "typescript": "4.5.4",
    "xo": "0.47.0"
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "bugfixes": true,
          "modules": false,
          "targets": {
            "esmodules": true
          }
        }
      ]
    ],
    "env": {
      "production": {
        "presets": [
          "babel-preset-power-assert"
        ]
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint-and-fix"
    ],
    "package.json": [
      "npm run lint-config-and-fix"
    ]
  },
  "prettier": {
    "trailingComma": "all"
  },
  "type": "module",
  "xo": {
    "prettier": true,
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "doc/**"
        ],
        "env": "browser"
      },
      {
        "files": [
          "src/transforms/**",
          "src/recipes/**"
        ],
        "rules": {
          "unicorn/filename-case": "off"
        }
      }
    ]
  }
}
