{
  "name": "rejuvenate",
  "description": "Keep your package up to date",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "author": "aureooms",
  "homepage": "https://aureooms.github.io/rejuvenate",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/rejuvenate"
  },
  "bugs": {
    "url": "https://github.com/aureooms/rejuvenate/issues"
  },
  "keywords": [
    "codemod",
    "dev",
    "esnext",
    "patch",
    "typescript",
    "update"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src --out-dir dist --source-maps",
    "build-docs": "esdoc",
    "build-gh-pages": "npm run build-docs",
    "commit-msg": "commitlint --edit",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "debug": "NODE_ENV=debug npm run test -- -st --fail-fast",
    "dev": "npm run lint-config && npm run lint-and-fix && npm run cover -- -- -st --fail-fast",
    "install-hooks": "husky install",
    "lint": "xo",
    "lint-and-fix": "npm run lint -- --fix",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np --message ':hatching_chick: chore(release): v%s'",
    "test": "ava",
    "travis": "npm run lint && npm run cover"
  },
  "dependencies": {
    "@aureooms/js-cardinality": "^2.0.0",
    "@aureooms/js-collections-deque": "^4.0.1",
    "@aureooms/js-compare": "^1.4.8",
    "@aureooms/js-itertools": "^5.0.0",
    "@aureooms/js-topological-sorting": "^0.2.1",
    "@babel/parser": "^7.13.11",
    "chalk": "^4.1.0",
    "cli-truncate": "^2.1.0",
    "command-exists": "^1.2.9",
    "del": "^6.0.0",
    "elegant-spinner": "^2.0.0",
    "execa": "^5.0.0",
    "fast-glob": "^3.2.5",
    "figures": "^3.2.0",
    "find-up": "^5.0.0",
    "indent-string": "^4.0.0",
    "listr": "^0.14.3",
    "load-json-file": "^6.2.0",
    "log-symbols": "^4.0.0",
    "log-update": "^4.0.0",
    "make-dir": "^3.1.0",
    "npm-check-updates": "^11.2.1",
    "pkg-dir": "^5.0.0",
    "power-assert": "1.6.1",
    "recast": "^0.20.4",
    "simple-git": "^2.36.1",
    "strip-ansi": "^6.0.0",
    "write-json-file": "^4.3.0",
    "yaml": "^1.10.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/register": "7.13.8",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "ava": "3.15.0",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.6.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "np": "7.4.0",
    "pinst": "2.1.6",
    "regenerator-runtime": "^0.13.7",
    "xo": "0.38.2"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": [
            "maintained node versions"
          ]
        }
      ]
    ],
    "env": {
      "debug": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": "current node"
            }
          ],
          "babel-preset-power-assert"
        ]
      },
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": "current node"
            }
          ],
          "babel-preset-power-assert"
        ]
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ]
      },
      "production": {
        "presets": [
          "babel-preset-power-assert"
        ]
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint-and-fix"
    ],
    "package.json": [
      "npm run lint-config-and-fix"
    ]
  },
  "prettier": {
    "trailingComma": "all"
  },
  "xo": {
    "prettier": true,
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "doc/**"
        ],
        "env": "browser"
      },
      {
        "files": [
          "src/transforms/**",
          "src/recipes/**"
        ],
        "rules": {
          "unicorn/filename-case": "off"
        }
      }
    ]
  }
}
