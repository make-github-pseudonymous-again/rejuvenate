{
  "name": "rejuvenate",
  "description": "Keep your package up to date",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "author": "make-github-pseudonymous-again",
  "homepage": "https://make-github-pseudonymous-again.github.io/rejuvenate",
  "repository": {
    "type": "git",
    "url": "https://github.com/make-github-pseudonymous-again/rejuvenate"
  },
  "bugs": {
    "url": "https://github.com/make-github-pseudonymous-again/rejuvenate/issues"
  },
  "keywords": [
    "codemod",
    "dev",
    "esnext",
    "patch",
    "typescript",
    "update"
  ],
  "sideEffects": false,
  "main": "dist/main.js",
  "bin": "dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src --out-dir dist --source-maps",
    "build-gh-pages": "npm run build-docs",
    "commit-msg": "commitlint --edit",
    "install-hooks": "husky",
    "lint": "xo",
    "lint-and-fix": "npm run lint -- --fix",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "precommit": "lint-staged --config .config/lint-staged/config.js",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np --message ':hatching_chick: chore(release): v%s'",
    "tsc": "tsc",
    "type-check": "npm run tsc -- --noEmit"
  },
  "dependencies": {
    "@async-iterable-iterator/async-iterable-to-array": "^0.0.1",
    "@babel/parser": "^7.23.9",
    "@comparison-sorting/is-sorted": "^0.0.1",
    "@data-structure/deque": "^9.0.1",
    "@graph-algorithm/topological-sorting": "^1.0.0",
    "@iterable-iterator/cardinality": "^4.0.0",
    "@iterable-iterator/filter": "^1.0.1",
    "@iterable-iterator/iter": "^1.0.1",
    "@iterable-iterator/list": "^1.0.1",
    "@iterable-iterator/map": "^1.0.1",
    "@iterable-iterator/mapping": "^9.0.0",
    "@iterable-iterator/reduce": "^1.0.1",
    "@iterable-iterator/slice": "^1.0.2",
    "@iterable-iterator/zip": "^1.0.1",
    "@total-order/primitive": "^1.0.1",
    "chalk": "^5.3.0",
    "cli-truncate": "^4.0.0",
    "command-exists": "^1.2.9",
    "del": "^7.1.0",
    "elegant-spinner": "^3.0.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "figures": "^6.0.1",
    "find-up": "^7.0.0",
    "indent-string": "^5.0.0",
    "listr": "^0.14.3",
    "load-json-file": "^7.0.1",
    "log-symbols": "^6.0.0",
    "log-update": "^6.0.0",
    "make-dir": "^4.0.0",
    "move-file": "^3.1.0",
    "npm-check-updates": "^16.14.14",
    "pkg-dir": "^8.0.0",
    "power-assert": "1.6.1",
    "recast": "^0.23.4",
    "simple-git": "^3.22.0",
    "strip-ansi": "^7.1.0",
    "total-order": "^4.0.1",
    "write-json-file": "^5.0.0",
    "yaml": "^2.3.4",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.24.1",
    "@babel/core": "7.24.4",
    "@babel/preset-env": "7.24.4",
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
    "@types/yargs": "17.0.32",
    "babel-preset-power-assert": "3.0.0",
    "fixpack": "4.0.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "np": "10.0.4",
    "pinst": "3.0.0",
    "regenerator-runtime": "0.14.1",
    "typescript": "5.4.5",
    "xo": "0.58.0"
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "bugfixes": true,
          "modules": false,
          "targets": {
            "esmodules": true
          }
        }
      ]
    ],
    "env": {
      "production": {
        "presets": [
          "babel-preset-power-assert"
        ]
      }
    }
  },
  "prettier": {
    "trailingComma": "all"
  },
  "type": "module",
  "xo": {
    "prettier": true,
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ],
      "unicorn/prevent-abbreviations": "off",
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "parent",
            "sibling",
            "index"
          ],
          "newlines-between": "always",
          "warnOnUnassignedImports": true
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "doc/**"
        ],
        "env": "browser"
      },
      {
        "files": [
          "src/transforms/**",
          "src/recipes/**"
        ],
        "rules": {
          "unicorn/filename-case": "off"
        }
      }
    ]
  }
}
